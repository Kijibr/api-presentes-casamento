{
  "compilerOptions": {
    "target": "es2016", 
    "module": "commonjs",   
    "esModuleInterop": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
